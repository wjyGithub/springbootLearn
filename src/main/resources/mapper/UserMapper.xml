<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.springboot.first.mapper.UserMapper">
    <select id="queryUser" resultType="com.springboot.first.domain.User">
        SELECT
          u.ID,
          u.NAME,
          u.AGE
        FROM user u
    </select>
</mapper>